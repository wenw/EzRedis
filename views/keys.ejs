<h3> Server: <%= ip %></h3>

<h4>Total has <%= count %> keys.</h4>
<table class="table table-hover">
<tr>
    <th style="width:10%">No.</th>
    <th style="width:65%">Key</th>
    <th style="width:10%">Type</th>
    <th style="width:15%">Action</th>
</tr>
<% keys.forEach(function(key, index){ %>
<tr>
    <td><%= index + 1 %></td>
    <td><%= key %></td>
    <td>
        <a href="/show/<%= key %>" class="btn btn-info">Show</a>&nbsp;
        <a href="/remove/<%= key %>" class="btn btn-danger" onclick="return confirm('Are you sure to delete this item.');">Remove</a>
    </td>
</tr>
<% }) %>
</table>








